{
  "name": "Vocabulary helper", 
  "description": "Sample Chrome Extension", 
  "version": "1.0",
  "manifest_version": 3, 
  "background": {
      "service_worker": "background.js",
      "type":"module"
  },
  "permissions": ["storage", "activeTab", "contextMenus"
],
  "action": {  "default_popup":"popup/popup.html"}, 
  "host_permissions": ["<all_urls>"]
}
